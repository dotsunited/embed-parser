{
    "name": "dotsunited/embed-parser",
    "description": "Simple utility to parse URLs in HTML or text content which are placed on its own line to turn them into embeds.",
    "keywords": ["embed", "oembed", "html", "parser"],
    "homepage": "https://github.com/dotsunited/embed-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Sorgalla",
            "email": "jan.sorgalla@dotsunited.de",
            "homepage": "https://dotsunited.de"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DotsUnited\\EmbedParser\\": "tests/"
        }
    }
}
